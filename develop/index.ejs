  <script>
    // Initialize Calendly button
    document.addEventListener('DOMContentLoaded', function() {
      // This will be called when the Calendly script is loaded
      if (typeof Calendly !== 'undefined') {
        // Calendly is already loaded
        initCalendly();
      } else {
        // Wait for Calendly to load
        const checkCalendly = setInterval(function() {
          if (typeof Calendly !== 'undefined') {
            clearInterval(checkCalendly);
            initCalendly();
          }
        }, 100);
      }
      
      function initCalendly() {
        // Initialize Calendly button
        const calendlyButton = document.getElementById('calendly-button');
        if (calendlyButton) {
          calendlyButton.addEventListener('click', function(e) {
            e.preventDefault();
            Calendly.initPopupWidget({
              url: 'https://calendly.com/ratna-singh-jp/30min',
              text: '面談を予約する',
              color: '#3a86ff',
              textColor: '#ffffff',
              branding: false
            });
            return false;
          });
        }
      }
    });
